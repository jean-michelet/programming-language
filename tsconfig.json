{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "noImplicitAny": true,
    "removeComments": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "outDir": "./dist",
    "sourceMap": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"],
}
